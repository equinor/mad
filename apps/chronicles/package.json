{
  "name": "@equinor/mad-chronicles",
  "version": "1.1.1",
  "scripts": {
    "start": "npx expo start --dev-client",
    "android": "npx expo run:android",
    "dev": "npx expo run:ios",
    "web": "npx expo start --web",
    "build": "npx expo prebuild --platform ios",
    "docker": "npx expo export -p web",
    "ios": "npx expo run:ios",
    "lint": "tsc --noEmit && eslint .",
    "clean": "rm -rf node_modules/ && rm -rf ios/",
    "detox:build": "npx detox build --configuration ios.release",
    "detox:test": "npx detox test --configuration ios.release",
    "use:dev": "sed -i \"s/currentEnvironment: \".*\"/currentEnvironment: \\\"dev\\\",/g\" mad.config.ts",
    "use:test": "sed -i \"s/currentEnvironment: \".*\"/currentEnvironment: \\\"test\\\",/g\" mad.config.ts",
    "use:prod": "sed -i \"s/currentEnvironment: \".*\"/currentEnvironment: \\\"prod\\\",/g\" mad.config.ts"
  },
  "dependencies": {
    "@equinor/mad-auth": "*",
    "@equinor/mad-components": "*",
    "@equinor/mad-core": "*",
    "@equinor/mad-dfw": "*",
    "@equinor/mad-navigation": "*",
    "@equinor/react-native-skia-draw": "*",
    "@expo/metro-runtime": "~3.2.1",
    "@expo/vector-icons": "^14.0.0",
    "@expo/webpack-config": "^19.0.0",
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "@shopify/react-native-skia": "1.2.3",
    "expo": "~51.0.14",
    "expo-asset": "~10.0.9",
    "expo-auth-session": "~5.5.2",
    "expo-constants": "~16.0.2",
    "expo-crypto": "~13.0.2",
    "expo-device": "~6.0.2",
    "expo-font": "~12.0.7",
    "expo-image-picker": "~15.0.5",
    "expo-linking": "~6.3.1",
    "expo-localization": "~15.0.3",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.6",
    "expo-web-browser": "~13.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.2",
    "react-native-device-info": "^10.9.0",
    "react-native-gesture-handler": "2.16.0",
    "react-native-msal": "git+https://github.com/equinor/react-native-msal.git#38c1bac756b8019e3b7f916893a7f588c811e407",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.7",
    "uuid": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@config-plugins/detox": "^7.0.0",
    "@equinor/eslint-config-mad": "*",
    "@types/react": "~18.2.43",
    "babel-loader": "^8.3.0",
    "detox": "^20.20.2",
    "jest": "^29",
    "react-test-renderer": "18.1.0",
    "typescript": "5.3.3"
  },
  "private": true
}
