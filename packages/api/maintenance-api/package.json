{
  "name": "@equinor/mad-maintenance-api-ts-wrapper",
  "description": "Typescript wrapper for the Equinor Maintenance API",
  "version": "1.15.1",
  "type": "module",
  "scripts": {
    "test": "jest",
    "generate": "openapi -o ./src/lib/generated -c axios --useUnionTypes --useOptions --exportCore false -i",
    "typecheck": "tsc --project tsconfig.lib.json --emitDeclarationOnly",
    "build": "node esbuild.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/equinor/mad/"
  },
  "keywords": [
    "API",
    "TypeScript",
    "wrapper"
  ],
  "author": {
    "name": "Mobile App Delivery team"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/equinor/mad/issues"
  },
  "homepage": "https://github.com/equinor/mad/",
  "devDependencies": {
    "openapi-typescript-codegen": "^0.23.0"
  },
  "dependencies": {
    "axios": "^1.3.2",
    "form-data": "4.x"
  },
  "files": [
    "CODE_OF_CONDUCT.md",
    "LICENSE",
    "CHANGELOG.md",
    "dist"
  ],
  "module": "./dist/index.esm.js",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts"
}