{
    "name": "@equinor/mad-core",
    "version": "0.6.1",
    "description": "Core library for the Mobile App Delivery team",
    "type": "module",
    "scripts": {
        "build": "tsup-node && echo '\n ⚙️ Generating typescript declarations..' && tsc --project tsconfig.json --emitDeclarationOnly --declaration",
        "test": "jest",
        "dev": "tsup-node --watch --clean=false",
        "lint": "eslint src/",
        "clean": "rm -rf node_modules/ && rm -rf dist/"
    },
    "keywords": [
        "react native",
        "mad",
        "expo",
        "equinor"
    ],
    "author": {
        "name": "Equinor Mobile App Delivery team (MAD)"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/equinor/mad/issues"
    },
    "homepage": "https://github.com/equinor/mad/",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "peerDependencies": {
        "@react-native-async-storage/async-storage": "*",
        "@react-navigation/native": "^6.1.8",
        "expo-device": ">=5.4.0",
        "expo-localization": ">=14.3.0",
        "react": ">=18.0",
        "react-dom": ">=18.0",
        "react-native": ">=0.70",
        "react-native-gesture-handler": "^2.14.0",
        "react-native-msal": "*"
    },
    "dependencies": {
        "@equinor/mad-auth": "*",
        "@equinor/mad-components": "*",
        "@equinor/mad-insights": "*",
        "@equinor/mad-navigation": "*",
        "@react-navigation/elements": "1.3.21",
        "markdown-it": "^14.0.0",
        "react-native-markdown-display": "^7.0.2",
        "zustand": "^4.4.1"
    },
    "devDependencies": {
        "@babel/core": "^7.21.4",
        "@equinor/eslint-config-mad": "*",
        "@testing-library/jest-native": "^5.4.3",
        "@testing-library/react": "^14.0.0",
        "@testing-library/react-native": "^12.3.0",
        "@types/showdown": "^2.0.0",
        "react-test-renderer": "18.1.0"
    },
    "files": [
        "CODE_OF_CONDUCT.md",
        "LICENSE",
        "CHANGELOG.md",
        "dist"
    ]
}