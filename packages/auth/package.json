{
    "name": "@equinor/mad-auth",
    "version": "0.1.24",
    "description": "A simple wrapper library for react-native-msal",
    "type": "module",
    "scripts": {
        "build": "tsup-node && echo '\n ⚙️ Generating typescript declarations..' && tsc --project tsconfig.json --emitDeclarationOnly --declaration",
        "test": "jest",
        "dev": "tsup-node --watch --clean=false",
        "lint": "eslint src/",
        "clean": "rm -rf node_modules/ && rm -rf dist/"
    },
    "keywords": [
        "react native",
        "authentication",
        "typescript",
        "msal"
    ],
    "author": {
        "name": "Equinor Mobile App Delivery team (MAD)"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/equinor/mad/issues"
    },
    "homepage": "https://github.com/equinor/mad/",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "peerDependencies": {
        "react": ">=18.0",
        "react-dom": ">=18.0",
        "react-native": ">=0.70"
    },
    "dependencies": {
        "@equinor/mad-components": "*",
        "expo-auth-session": "~5.0.2",
        "expo-crypto": "~12.4.1",
        "jwt-decode": "^4.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.21.4",
        "@equinor/eslint-config-mad": "*",
        "@testing-library/jest-native": "^5.4.3",
        "@testing-library/react": "^14.0.0",
        "@testing-library/react-native": "^12.3.0",
        "@types/showdown": "^2.0.0",
        "react-test-renderer": "18.2.0"
    },
    "files": [
        "CODE_OF_CONDUCT.md",
        "LICENSE",
        "CHANGELOG.md",
        "dist"
    ]
}
