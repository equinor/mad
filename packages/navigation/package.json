{
    "name": "@equinor/mad-navigation",
    "version": "0.2.0",
    "description": "Navigation package built on top of React Navigation",
    "type": "module",
    "scripts": {
        "build": "tsup-node && echo '\n ⚙️ Generating typescript declarations..' && tsc --project tsconfig.json --emitDeclarationOnly --declaration",
        "test": "jest",
        "dev": "tsup-node --watch --clean=false",
        "lint": "eslint src/",
        "clean": "rm -rf node_modules/ && rm -rf dist/"
    },
    "keywords": [
        "react native",
        "react navigation",
        "typescript",
        "navigation"
    ],
    "author": {
        "name": "Equinor Mobile App Delivery team (MAD)"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/equinor/mad/issues"
    },
    "homepage": "https://github.com/equinor/mad/",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "dependencies": {
        "@react-navigation/elements": "1.3.21"
    },
    "peerDependencies": {
        "react": ">=18.0",
        "react-dom": ">=18.0",
        "react-native": ">=0.70",
        "@react-navigation/native": ">=6",
        "@react-navigation/native-stack": ">=6",
        "@react-navigation/bottom-tabs": ">=6",
        "@react-navigation/elements": "1.3.21"
    },
    "devDependencies": {
        "@equinor/eslint-config-mad": "workspace:*"
    },
    "files": [
        "CODE_OF_CONDUCT.md",
        "LICENSE",
        "CHANGELOG.md",
        "dist"
    ]
}
