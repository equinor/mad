{
    "$schema": "https://turbo.build/schema.json",
    "globalDependencies": ["**/.env.*local"],
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", "build/**", "ios/**"]
        },
        "lint": {
            "outputs": [],
            "cache": false
        },
        "dev": {
            "cache": false
        },
        "web": {
            "cache": false
        },
        "generate": {
            "cache": false
        },
        "test": {},
        "clean": {
            "cache": false
        },
        "docker": {
            "outputs": ["dist/**"]
        },
        "use:dev": {},
        "use:test": {},
        "use:prod": {}
    }
}
